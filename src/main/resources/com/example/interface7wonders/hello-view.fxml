    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="emptyText" strokeType="OUTSIDE" strokeWidth="0.0" />
   <Text fx:id="emptyText2" strokeType="OUTSIDE" strokeWidth="0.0" />
    <Button fx:id="playButton" minHeight="50.0" minWidth="125.0" onAction="#onPlayButtonClick" stylesheets="@../../../stylesheet.css" text="Play" textAlignment="CENTER">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
